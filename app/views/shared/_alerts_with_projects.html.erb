<div class="panel panel-info">
  <table class='table table-responsive'>
    <thead>
      <tr>
        <td> Project </td>
        <td> Alert </td>
      </tr>
    </thead>

    <tbody>
      <% alerts.each do |alert| %>
        <tr>
          <td><%= alert.project.name %></td>
          <td><%= alert.name %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>